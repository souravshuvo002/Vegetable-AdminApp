        <activity android:name=".Activity.WalletActivity" />
        <activity android:name=".Activity.UserAllOrderHistoryActivity" />
        <activity android:name=".Activity.AllUsersActivity" />
        <activity android:name=".Activity.SendNotificationActivity" />
        <activity android:name=".Activity.UpdateCouponActivity" />
        <activity android:name=".Activity.UpdateFoodsActivity" />
        <activity android:name=".Activity.UpdateMenuActivity" />
        <activity android:name=".Activity.UpdateBannerActivity" />
        <activity android:name=".Activity.SlotActivity" />
